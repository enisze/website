---
import Component from './Component.astro'
---

<Component>
    <section class="education" id="education">
        <h2 class="section__title">Education</h2>
        <ul class="education__list">
            <li>
                <h3 class="education__degree">Master of Computer Science</h3>
                <p class="education__university">Aachen University</p>
                <p class="education__date">2016 - 2018</p>
                <ul class="education__courses">
                    <li>RDF Schema Generation</li>
                    <li>Artificial Intelligence</li>
                    <li>Data Mining</li>
                </ul>
            </li>
            <li>
                <h3 class="education__degree">Bachelor of Computer Science</h3>
                <p class="education__university">Cologne University</p>
                <p class="education__date">2012 - 2016</p>
                <ul class="education__courses">
                    <li>Generative Development</li>
                    <li>Machine Learning</li>
                    <li>Data Science</li>
                </ul>
            </li>
        </ul>
    </section>
</Component>
