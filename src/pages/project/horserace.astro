---
import Skill from '../../components/Skill.astro'
import { getItemsByLabels, type StackLabel } from '../../components/stack'
import Layout from '../../layouts/Layout.astro'

import { Image } from 'astro:assets'

const labels: StackLabel[] = ['Typescript', 'Expo', 'React Native']

const skills = getItemsByLabels(labels)
---

<Layout>
    <div>
        <div>test</div>
        <Image
            src="/images/horserace.jpg"
            alt="Event Wizard User Screenshot"
            width={932}
            height={910}
            class={`rounded-xl md:rounded-r-none md:max-w-[40vw] md:h-full`}
            decoding="async"
            format="avif"
            loading="lazy"
        />

        <Image
            src="/images/horseRace2.png"
            alt="Event Wizard User Screenshot"
            width={932}
            height={910}
            class={`rounded-xl md:rounded-r-none md:max-w-[40vw] md:h-full`}
            decoding="async"
            format="avif"
            loading="lazy"
        />

        {skills.map((item) => <Skill item={item} />)}
    </div>
</Layout>
